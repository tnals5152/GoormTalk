                },
                "id": {
                    "type": "integer"
                },
                "owner": {
                    "$ref": "#/definitions/models.User"
                },
                "roomName": {
                    "description": "room_name",
                    "type": "string"
                },
                "roomType": {
                    "type": "integer"
                },
                "updatedAt": {
                    "type": "string"
                },
                "userID": {
                    "type": "integer"
                }
            }
        },
        "models.RoomUser": {
            "type": "object",
